{
  "name": "RNReduxPaper",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "android": "npx react-native run-android",
    "android-release-aab": "cd android && ./gradlew bundleRelease",
    "android-release-apk": "cd android && ./gradlew assembleRelease",
    "android-release-test": "npx react-native run-android --variant release",
    "ios": "npx react-native run-ios",
    "ios-release": "npx react-native run-ios --configuration Release",
    "ios-device": "npx react-native run-ios --device",
    "ios-device-release": "npx react-native run-ios --device --configuration Release",
    "ios-list-sim": "xcrun simctl list devices",
    "start": "npx react-native start",
    "start-rc": "npx react-native start --reset-cache",
    "postinstall": "cd ios && pod install",
    "clean": "rm -rf ~/Library/Developer/Xcode/DerivedData && rm -rf node_modules/ /tmp/metro-*  && watchman watch-del-all && watchman shutdown-server && yarn cache clean",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "deep-clean": "rm -rf ~/Library/Developer/Xcode/DerivedData ios/build ios/Pods && cd android && ./gradlew clean && cd .. && rm -rf node_modules/ /tmp/metro-* && watchman watch-del-all && watchman shutdown-server && yarn cache clean",
    "reset": "yarn clean && yarn install",
    "deep-reset": "yarn deep-clean && yarn install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "react": "18.2.0",
    "react-native": "0.72.1",
    "react-native-paper": "^5.9.1",
    "react-native-safe-area-context": "^4.6.4",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.7",
    "@tsconfig/react-native": "^3.0.0",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.5",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
